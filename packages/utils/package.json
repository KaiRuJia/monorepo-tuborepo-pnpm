{
  "name": "@pawpatrol/utils",
  "version": "1.0.0",
  "description": "A collection of utility functions",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./string": {
      "require": "./dist/string.js",
      "import": "./dist/string.mjs",
      "types": "./dist/string.d.ts"
    },
    "./number": {
      "require": "./dist/number.js",
      "import": "./dist/number.mjs",
      "types": "./dist/number.d.ts"
    },
    "./demo": {
      "require": "./dist/demo.js",
      "import": "./dist/demo.mjs",
      "types": "./dist/demo.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "echo \"Error: no test specified\" && exit 1",
    "debug": "tsx watch src/debug.ts",
    "start": "tsx src/debug.ts",
    "changelog": "conventional-changelog -p conventionalcommits -i CHANGELOG.md -s",
    "changelog:init": "conventional-changelog -p conventionalcommits -i CHANGELOG.md -s -r 0"
  },
  "keywords": [
    "utils",
    "utilities"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.20.0",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "conventional-changelog-cli": "^5.0.0",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.3.0",
    "vite": "^7.2.6",
    "vite-plugin-dts": "^3.7.0"
  }
}
